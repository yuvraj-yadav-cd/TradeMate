{
  "expo": {
    "name": "TradeMate",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.yuvraj_yadav.myapp"
    },
    "web": {
      "favicon": "./assets/splash-icon.jpg"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you pick a profile picture."
        }
      ],
      [
        "expo-splash-screen",
        {
          "preventAutoHide": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "347f894b-c77a-432b-bbd8-8be4f9f6461e"
      }
    }
  }
}
